{
    "name": "gamegos/consul-imex",
    "description": "Import/Export Tool for Consul Key/Value Storage",
    "keywords": [
        "consul importer",
        "consul exporter",
        "json to consul",
        "consul to json"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Gamegos\\ConsulImex\\": "src/"
        }
    },
    "bin" : [
        "scripts/consul-imex"
    ],
    "require-dev": {
        "gamegos/php-code-sniffer": "*"
    },
    "authors": [
        {
            "name": "Safak Ozpinar",
            "email": "safak@gamegos.com"
        }
    ]
}
